//
// Call APIs
const log = require('logToConsole');
const createQueue = require('createQueue');
const callInWindow = require('callInWindow');
const aliasInWindow = require('aliasInWindow');
const copyFromWindow = require('copyFromWindow');
const setInWindow = require('setInWindow');
const injectScript = require('injectScript');
const makeTableMap = require('makeTableMap');
const getUrl = require('getUrl');
const getReferrerUrl = require('getReferrerUrl');
const JSON = require('JSON');
const encodeUri = require('encodeUri');
const getQueryParameters = require('getQueryParameters');
const getReferrerQueryParameters = require('getReferrerQueryParameters');
//
// Get ONEDot Object and Q
const getONEdot = () => {
  // Return the existing 'fbq' global method if available
  const ONEdot = copyFromWindow('ONEdot');
  const ONEdotQ = copyFromWindow('ONEdot.q');
  //
  if (ONEdot) {
    return ONEdot;
  }
  //
  setInWindow('ONEdot', function() {    
    const callMethod = copyFromWindow('ONEdot.enqueue');
    if (callMethod) {           
      callInWindow('ONEdot.enqueue', null, arguments); 
    } else {       
      callInWindow('ONEdot.q.push', arguments);
    }
  });
  
  // Create the fbq.queue
  if (!ONEdotQ) {
    createQueue('ONEdot.q');
  }
    
  // Return the global 'fbq' method, created above
  return copyFromWindow('ONEdot');
};
//
const ONEdot = getONEdot();
//
ONEdot('init', data.dotId, {debug: data.dotSettingsDebugMode, limitedDataUse: data.dotSettingsLDU, logs: data.dotSettingsConsoleOutput});
if(data.deployMetaPixel == true){
  ONEdot('init-meta', data.metaPixelId, {});
}
//
const pageUrl = getUrl();
const pageReferrer = getReferrerUrl();
const eventData = data.eventData ? makeTableMap(data.eventData, 'name', 'value') : {};
const command = data.eventName !== 'Custom' ? 'trackSingle' : 'trackSingleCustom';
const eventName = data.eventName !== 'Custom' ? data.eventName : data.customEventName;
//
const utms = {
  utm_source: getQueryParameters('utm_source', false),
  utm_medium: getQueryParameters('utm_medium', false),
  utm_campaign: getQueryParameters('utm_campaign', false),
  utm_term: getQueryParameters('utm_term', false),
  utm_content: getQueryParameters('utm_content', false),
  utm_aecid: getQueryParameters('utm_aecid', false),
  utm_placement: getQueryParameters('utm_placement', false),
  utm_site_source_name: getQueryParameters('utm_site_source_name', false),
  fbclid: getQueryParameters('fbclid', false),
  gclid: getQueryParameters('gclid', false)
};
const referrerUtms = {
  utm_source: getReferrerQueryParameters('utm_source', false),
  utm_medium: getReferrerQueryParameters('utm_medium', false),
  utm_campaign: getReferrerQueryParameters('utm_campaign', false),
  utm_term: getReferrerQueryParameters('utm_term', false),
  utm_content: getReferrerQueryParameters('utm_content', false),
  utm_aecid: getReferrerQueryParameters('utm_aecid', false),
  utm_placement: getReferrerQueryParameters('utm_placement', false),
  utm_site_source_name: getReferrerQueryParameters('utm_site_source_name', false),
  fbclid: getReferrerQueryParameters('fbclid', false),
  gclid: getReferrerQueryParameters('gclid', false)
};
//
eventData.url = pageUrl;
eventData.referrer = pageReferrer;
eventData.utm = utms;
eventData.referrerUtm = referrerUtms;
eventData.triggerMetaEvent = data.triggerMetaEvent;
eventData.metaEventName = data.metaEventName !== 'Custom' ? data.metaEventName : data.metaCustomEventName;
//
if(eventName){ ONEdot('track', data.eventName, eventData); }
//
injectScript('https://myonedot.co/ingest/events/web2', data.gtmOnSuccess, data.gtmOnFailure, 'ONEDotPixel');
// Call data.gtmOnSuccess when the tag is finished.
data.gtmOnSuccess();
//


